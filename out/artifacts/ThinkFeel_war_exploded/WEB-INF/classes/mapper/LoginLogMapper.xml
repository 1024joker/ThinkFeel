<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mxthd.dao.LoginLogMapper">
    <insert id="addLog" parameterType="com.mxthd.bean.User">
        insert into login_log(uid,ip,device) values(#{uid},#{ip},#{device})
    </insert>
</mapper>