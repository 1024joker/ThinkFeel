<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mxthd.dao.OptionsMapper">
    <select id="findByKey" parameterType="string" resultType="com.mxthd.bean.Options">
        select * from options where option_key=#{optionKey}
    </select>
</mapper>